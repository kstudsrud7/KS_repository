  <DataSets>
    <DataSet Name="MailingLabels">
      <Query>
        <DataSourceName>MSCRM_FetchDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="CRM_contact">
            <Value>=Parameters!CRM_contact.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>&lt;fetch version="1.0" output-format="xml-platform" mapping="logical" distinct="true" &gt;
    &lt;entity name="contact" enableprefiltering="true"&gt;
        &lt;attribute name="fullname" /&gt;
        &lt;order attribute="fullname" descending="false" /&gt;
        &lt;filter type="and" &gt;
            &lt;condition attribute="statuscode" operator="eq" value="1" /&gt;
        &lt;/filter&gt;
        &lt;link-entity name="ait_physicaladdress" from="ait_contact" to="contactid" alias="pa" &gt;
            &lt;attribute name="ait_address" /&gt;
            &lt;filter type="and" &gt;
                &lt;condition attribute="ait_mailing" operator="eq" value="1" /&gt;
                &lt;condition attribute="statuscode" operator="eq" value="1" /&gt;
            &lt;/filter&gt;
        &lt;/link-entity&gt;
    &lt;/entity&gt;
&lt;/fetch&gt;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="fullname">
          <DataField>fullname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pa_ait_address">
          <DataField>pa_ait_address</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
